{
    "name": "app",
    "scripts": {
        "build:assets": "bun build app/assets/ --outdir app/static/app/ --minify --target bun",
        "build:ts": "bun build $(find app/static/app/ts/ -name '*.ts' | sort) --outdir app/static/app/js --outbase app/static/app/ts/ --minify --sourcemap --target bun",
        "build": "bun run build:assets && bun run build:ts"
    },
    "dependencies": {"@fontsource/rubik": "^5.2.6",
        "@fortawesome/fontawesome-free": "^7.0.0",
        "@phosphor-icons/web": "^2.1.2",
        "@selectize/selectize": "^0.15.2",
        "bootstrap": "^5.3.7",
        "d3": "^7.9.0",
        "datatables.net-bs5": "^2.3.3",
        "datatables.net-select-bs5": "^3.0.1",
        "ion-rangeslider": "^2.3.1",
        "jquery": "^3.7.1",
        "vega": "^6.1.2",
        "vega-embed": "^7.0.2",
        "vega-lite": "^6.2.0"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "private": true,
    "type": "module"
}
